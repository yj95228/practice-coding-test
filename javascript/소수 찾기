function solution(n){
    const sqrt = Math.floor(Math.sqrt(n) + 1);
    let answer = 0;
    let primeList = [...Array(n+1).keys()];

    for (let i = 2; i < sqrt; i++) {
        if (primeList[i]) {
            for (let j = i * i; j < n + 1; j += i) {
                primeList[j] = 0;
            }
        }
    }
    for (let i = 2; i < n+1; i++) {
        if (primeList[i]) {
          answer++;
        }
    }

    return answer;
}
