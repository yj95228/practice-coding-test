# https://programmers.co.kr/learn/courses/30/lessons/12903

def solution(s):
    answer = ''
    strlen = len(list(s))
    print(strlen)
    if strlen % 2 == 1:
           answer = list(s)[int(strlen/2)]
    else:
           answer = ''.join(list(s)[int(strlen/2-1):int(strlen/2+1)])
    return answer
